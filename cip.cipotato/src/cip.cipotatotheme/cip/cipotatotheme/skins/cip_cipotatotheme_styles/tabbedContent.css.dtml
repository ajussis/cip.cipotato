.tabslider ul {
float:left;
margin:0 0 0 -34px;
padding:0;
width:560px;
}
.tabslider ul a {
	color: #ffffff;
	text-decoration: none;
}

.tabslider ul a:hover {
	color: #aaaaaa;
}

.tabslider ul li {
	padding-bottom: 7px;
}